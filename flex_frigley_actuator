//for 35.40 flex
int a;
void setup()
{
  Serial.begin(2400);
  pinMode(A1, INPUT);
  pinMode(5,OUTPUT);
  pinMode(6,OUTPUT);
  
}
void loop()
{
a= analogRead(A1);
Serial.println(a);
delay(500);
/* front bend (>145)
 *  back bend ( < )
 *  normal(118-129)
 */
//5 input
//6 input
 if(a>145)
 {
  digitalWrite(5, LOW);
  digitalWrite(6, HIGH);
 }
 else if(a>=118 && a<130)
 {
  digitalWrite(5, HIGH);
  digitalWrite(6,LOW);
 }
 else
 {
  digitalWrite(5,LOW);
  digitalWrite(6,LOW);
 }

}
